{
  "name": "markdown-parser",
  "version": "0.0.1",
  "description": "",
  "main": "src/lib/index.ts",
  "scripts": {
    "build": "npm run clean && ./node_modules/typescript/bin/tsc -p ./src/lib/tsconfig.json",
    "demo": "npm run build && parallelshell \"npm run demo:watch\" \"npm run demo:serve\"",
    "demo:serve": "./node_modules/.bin/http-server ./dist -a localhost -p 4200 -c-1",
    "demo:assets": "cp ./src/demo-app/index.html ./dist/index.html && cp -r ./src/demo-app/assets ./dist",
    "demo:build": "./node_modules/typescript/bin/tsc -p ./src/demo-app/tsconfig.json",
    "demo:watch": "watch 'npm run demo:build && npm run demo:assets' ./src/demo-app",
    "asdfsa": "parallelshell \"npm run demo:watch\" \"npm run demo:serve\"",
    "test": "test",
    "lint": "./node_modules/tslint/bin/tslint -c tslint.json ./src/**/*.ts",
    "clean": "rm -rf dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thomaspink/markdown-parser.git"
  },
  "keywords": [
    "markdown",
    "parser"
  ],
  "author": "Thomas Pink",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/thomaspink/markdown-parser/issues"
  },
  "homepage": "https://github.com/thomaspink/markdown-parser#readme",
  "devDependencies": {
    "http-server": "^0.9.0",
    "parallelshell": "^2.0.0",
    "tslint": "^4.2.0",
    "typescript": "^2.1.4",
    "watch": "^1.0.1"
  }
}
